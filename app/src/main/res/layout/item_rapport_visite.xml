<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center">

        <TextView
            android:id="@+id/tvItemNumRapport"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Num Rapport  "/>

        <TextView
            android:id="@+id/tvItemNomPraticien"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nom praticien  "/>

        <TextView
            android:id="@+id/tvItemVillePraticien"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ville praticien  "/>

        <TextView
            android:id="@+id/tvItemDateVisite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date Visite"/>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>